{
  "name": "nextjs-app-starterkit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean-cache": "rimraf -rf ./node_modules/.cache",
    "dev": "npm run lint && npm run clean-cache && node index.js",
    "local": "npm run lint && npm run clean-cache && cross-env CONFIG_ENV=local node index.js",
    "local-no-lint": "npm run clean-cache && cross-env CONFIG_ENV=local node index.js",
    "build": "next build",
    "start": "cross-env NODE_ENV=production node index.js",
    "lint": "eslint --fix --ext .js pages src",
    "create-comp": "hygen generator component",
    "create-page": "hygen generator page",
    "create-reducer": "hygen generator reducer",
    "heroku-postbuild": "npm run build",
    "prestart": "npm run build",
    "analyze": "cross-env BUNDLE_ANALYZE=both npm run build",
    "build:static_export": "rimraf -rf ./out && npm run build && next export",
    "build:server-static": "npm run build:static_export && http-server ./out",
    "server-static": "http-server ./out",
    "now-login": "now login",
    "now-deploy": "now --prod"
  },
  "dependencies": {
    "@material-ui/core": "latest",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/styles": "latest",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "clsx": "latest",
    "cross-env": "^6.0.3",
    "dotenv-webpack": "^1.7.0",
    "express": "^4.17.1",
    "immer": "^3.2.0",
    "immutability-helper": "^3.0.1",
    "install": "^0.13.0",
    "lodash": "^4.17.15",
    "material-ui-popup-state": "^1.4.1",
    "next": "latest",
    "next-compose-plugins": "^2.2.0",
    "next-i18next": "^1.2.1",
    "next-redux-saga": "^4.0.2",
    "next-redux-wrapper": "^3.0.0-alpha.3",
    "node-sass": "^4.12.0",
    "npm": "^6.11.1",
    "produce": "^0.4.1",
    "prop-types": "latest",
    "react": "latest",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "latest",
    "react-dynamic-swiper": "^2.0.2",
